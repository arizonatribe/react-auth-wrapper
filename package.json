{
  "name": "react-auth-wrapper",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "description": "A utility library for handling authentication and authorization for react apps",
  "scripts": {
    "docs": "jsdoc --configure jsdoc.json",
    "docs:server": "live-server --open=docs --watch=assets,docs --port=5000",
    "prestart:docs": "npm run docs",
    "start:docs": "concurrently \"npm-watch\" \"npm run docs:server\"",
    "build": "node scripts/build/run",
    "lint": "eslint src test",
    "prepare": "npm run build",
    "test:watch": "nodemon test/index.js",
    "test:coverage": "nyc tape --require=jsdom --require=jsdom-global/register --require=mock-local-storage --require=@babel/register test/index.js | tap-nyc",
    "test": "tape --require=jsdom --require=jsdom-global/register --require=mock-local-storage --require=@babel/register test/index.js | faucet",
    "start": "npm run test:watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arizonatribe/react-auth-wrapper.git"
  },
  "authors": [
    "Matthew Russell",
    "David Nunez <arizonatribe@gmail.com>"
  ],
  "license": "MIT",
  "peerDependencies": {
    "react": ">=15.0.0 <17.0.0",
    "prop-types": ">=15.0.0 <16.0.0",
    "react-router": ">=4.0.0 <5.0.0",
    "react-dom": ">=15.0.0 <17.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.2.0",
    "@babel/core": "7.2.2",
    "@babel/plugin-external-helpers": "7.0.0",
    "@babel/plugin-proposal-class-properties": "7.2.1",
    "@babel/plugin-proposal-decorators": "7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "7.2.0",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-stage-0": "7.0.0",
    "@babel/register": "7.0.0",
    "babel-eslint": "8.2.1",
    "babel-plugin-add-module-exports": "1.0.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.13",
    "chalk": "2.4.1",
    "concurrently": "4.0.1",
    "cross-env": "5.2.0",
    "eslint": "5.5.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-defaults": "9.0.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.1",
    "extend-tape": "1.2.0",
    "faucet": "0.0.1",
    "fs-extra": "7.0.0",
    "jsdoc": "3.5.5",
    "jsdom": "13.1.0",
    "jsdom-global": "3.0.2",
    "live-server": "1.2.1",
    "mock-local-storage": "1.1.8",
    "nodemon": "1.18.7",
    "npm-watch": "0.4.0",
    "nyc": "13.1.0",
    "prop-types": "15.5.0",
    "react": "16.5.1",
    "react-dom": "16.5.1",
    "react-router-dom": "4.3.1",
    "react-test-renderer": "16.7.0",
    "rollup": "0.66.2",
    "rollup-plugin-babel": "4.0.3",
    "rollup-plugin-commonjs": "9.2.0",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-globals": "1.4.0",
    "rollup-plugin-node-resolve": "3.4.0",
    "rollup-plugin-replace": "2.0.0",
    "rollup-plugin-terser": "3.0.0",
    "tap-nyc": "1.0.3",
    "tape": "4.9.1",
    "tui-jsdoc-template": "1.2.2"
  },
  "dependencies": {
    "hoist-non-react-statics": "2.5.0"
  }
}
