{
  "presets": [
    "@babel/preset-react",
    ["@babel/preset-env", {
      "loose": true,
      "targets": {
        "browsers": ["last 2 versions"]
      }
    }]
  ],
  "plugins": [
    "@babel/proposal-object-rest-spread",
    ["transform-react-remove-prop-types", { "removeImport": true }],
    ["@babel/plugin-proposal-decorators", { "legacy": true }]
  ],
  "env": {
    "cjs": {
      "plugins": [
        "@babel/proposal-object-rest-spread",
        ["transform-react-remove-prop-types", { "removeImport": true }],
        ["@babel/plugin-proposal-decorators", { "legacy": true }],
        "add-module-exports"
      ]
    },
    "umd": {
      "presets": [
        ["@babel/preset-env", {
          "targets": {
            browsers: ['ie >= 11']
          }
        }]
      ]
    }
  }
}
